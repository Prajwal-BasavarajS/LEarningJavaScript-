// const prompt = require("prompt-sync")();

let n = 5;

let arr=[];
for (let i = 0; i < n; i++) {
    arr [i] = i+1;
}

console.log(arr);

let summ = arr.reduce((res,curr)=>{
    return res + curr ;
});


console.log(summ);


let prdt = arr.reduce((res,curr)=>{
    return res * curr ;
});

console.log(prdt);
