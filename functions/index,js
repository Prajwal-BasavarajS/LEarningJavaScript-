const countVowels = (str) =>
{
    let count = 0;
    for(let ch of str){
        if(ch === 'a' || ch === 'e' || ch === 'i' || ch === 'o'|| ch === 'u')
            count++;
        // console.log(str[ch]);
    }
    console.log( count );
}

countVowels("thejashwini T J");


let arr1 = [2,35,7,85,43,4];

function calcSq(num){
    console.log( num * num);
}

arr1.forEach(calcSq);


